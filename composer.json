{
  "name": "log1x/blade-svg-sage",
  "type": "package",
  "license": "MIT",
  "description": "Composer package to add support for Blade SVG by Adam Wathan to Roots Sage.",
  "homepage": "https://github.com/log1x/blade-svg-sage",
  "authors": [
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me"
    }
  ],
  "keywords": [
    "roots",
    "sage",
    "wordpress",
    "blade",
    "svg"
  ],
  "support": {
    "issues": "https://github.com/Log1x/blade-svg-sage/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dukenst2006/blade-svg"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "nothingworks/blade-svg": "dev-main"
  },
  "autoload": {
    "files": [
      "src/BladeSvgSage.php",
      "src/helpers.php"
    ]
  }
}
